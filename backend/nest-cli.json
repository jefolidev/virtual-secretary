{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "infra/main",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "./prisma/generated/",
        "outDir": "dist/generated",
        "watchAssets": true
      }
    ]
  }
}